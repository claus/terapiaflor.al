{
    "presets": [
        "next/babel",
        [
            "@babel/preset-env",
            {
                "corejs": "2",
                "useBuiltIns": "entry"
            }
        ]
    ],
    "plugins": [
        "transform-class-properties",
        [
            "module-resolver",
            {
                "root": ["./"],
                "alias": {
                    "components": "./components",
                    "pages": "./pages",
                    "static": "./static",
                    "styles": "./styles",
                    "utils": "./utils",
                    "hooks": "./hooks",
                }
            }
        ]
    ]
}
